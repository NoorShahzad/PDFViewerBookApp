<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookApp">
        <activity android:name=".weather"></activity>
        <activity android:name=".shapes" />
        <activity android:name=".seasons" />
        <activity android:name=".partsofbody" />
        <activity android:name=".daysofweek" />
        <activity android:name=".counting" />
        <activity android:name=".animals" />
        <activity android:name=".colors" />
        <activity android:name=".Booklist" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>